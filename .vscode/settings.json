{
    "editor.defaultFormatter": "TabNine.tabnine-vscode",
    "editor.defaultFoldingRangeProvider": "TabNine.tabnine-vscode",
    "clangd.path": "/usr/lib/llvm/17/bin/clangd",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "cmake.useCMakePresets": "always",
    "cmake.enableTraceLogging": true,
    "cmake.configureOnOpen": true,
    "cmake.options.advanced": {
        "build": {
            "statusBarVisibility": "visible"
        },
        "launch": {
            "statusBarVisibility": "visible"
        },
        "debug": {
            "statusBarVisibility": "visible"
        }
    },
    "C_Cpp.default.forcedInclude": [
        "/usr/lib/qt6/mkspecs/linux-clang/qplatformdefs.h"
    ]
}